aspect NewExpCounter {

    public int ASTNode.countNumberOfNewCogExpr() {
        int count = 0;
        for (int i = 0; i < getNumChild(); i++) {
            count += getChild(i).countNumberOfNewCogExpr();
        }
        return count;
    }

    public int NewExp.countNumberOfNewCogExpr() {
        return hasCog() ? 1 : 0;
    }

    syn lazy int Block.getNumberOfNewCogExpr() = countNumberOfNewCogExpr();
    

}