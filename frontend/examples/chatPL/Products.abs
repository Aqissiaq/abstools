/* 
 * Runtime product reconfiguration path:
 * LowEndChat <--> RegularChat <--> HighEndChat
 * 
 */
product HighEndChat (TextChat, VoiceChat, VideoChat, FileTransfer) {
    RegularChat by Upd_High2Reg;
}

product RegularChat (TextChat, VoiceChat) {
    HighEndChat by Upd_Reg2High;
    LowEndChat by Upd_Reg2Low;
}

product LowEndChat (TextChat) {
    RegularChat by Upd_Low2Reg;
}
