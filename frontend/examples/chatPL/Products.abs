/*
 * Runtime product reconfiguration path:
 * LowEnd <--> Regular <--> HighEnd
 *
 */

product LowEnd (Text) {
    Regular with DVoice by Upd_Low2Reg;
}

product Regular (Text, Voice) {
    HighEnd with DVideo,DFiles by Upd_Reg2High;
    LowEnd with DNoVoice by Upd_Reg2Low;
}

product HighEnd (Text, Voice, Video, Files) {
    Regular with DNoFiles,DNoVideo by Upd_High2Reg;
}

product HighStatic(Text, Voice, Video, Files);

