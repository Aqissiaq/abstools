module FLI.SystemUtils;

export *;

import * from ABS.FLI;

interface System {
	Unit outPrint(String s);
	Unit outPrintln(String s);
}

[Foreign] 
class System implements System {
	List<String> histories = Nil;
	
	Unit outPrint(String s) { 
		histories = appendright(histories,s); 
	}
	
	Unit outPrintln(String s) { 
		histories = appendright(histories,s); 
	}
}