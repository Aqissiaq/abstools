aspect DumpTree {

	//Added by ATO: 
	public void ASTNode.dumpTree(String indent, java.io.PrintStream pStream) {
		pStream.println(indent + this.toString());
		String childIndent = indent + "  ";
		for(int i = 0; i < getNumChild(); i++)
			getChildNoTransform(i).dumpTree(childIndent, pStream);
	}

//	public void VarOrFieldDecl.dumpTree(String indent, PrintStream stream) {
//	    stream.println(indent + this.toString() + "\t\t" + this.getName());
//	}


	public void Attribute.dumpTree(String indent, java.io.PrintStream pStream) {
	   	pStream.print(indent + this.getName() + ": "); 
	   	this.getAType().dumpTree("",pStream);
	}
	public void IntType.dumpTree(String indent, java.io.PrintStream pStream) {
	   	pStream.println(indent + "Int" + "["+getMin()+".."+getMax()+"]");
	}
	public String Limit.toString() {
	  return "*";
	}
	public String BoundaryVal.toString() {
	  return ""+getValue();
	}
	public void BoolType.dumpTree(String indent, java.io.PrintStream pStream) {
	   	pStream.println(indent + "Bool");
	}
	public void IntVal.dumpTree(String indent, java.io.PrintStream pStream) {
	   	pStream.println(indent + "Int[" +  this.getValue() + "]");
	}
	public void BoolVal.dumpTree(String indent, java.io.PrintStream pStream) {
	   	pStream.println(indent + "Bool[" +  this.getValue() + "]");
	}
	public void AttVar.dumpTree(String indent, java.io.PrintStream pStream) {
	   	pStream.println(indent + "AttVar[" +  this.getAName() + "]");
	}
	public void FeatVar.dumpTree(String indent, java.io.PrintStream pStream) {
	   	pStream.println(indent + "FeatVar[" +  this.getFName() + "]");
	}
	public void FAVar.dumpTree(String indent, java.io.PrintStream pStream) {
	  pStream.println(indent+"FAVar["+this.getFName()+"."+this.getAName()+"]");
	}

}  
