root Sandbox {
group allof {
  Disk {group oneof { Mem } } ,
  Mem { Int size; Value.asd == 5 + asd;}
}
Int car;
Bool couch;

car == ~4;
ifout: Mem.size >= (32 * 34) + 3 ;
}

extension Mem {
  Int speed;
  Bool size;
  requires: Disc;
  ~(true + false);
  Sandbox.couch == Sandbox.car;
  5 == Disk;
  Disc == Sandbox.couch;
}
